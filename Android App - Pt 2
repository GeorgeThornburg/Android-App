        setContentView(R.layout.activity_2);

//################################## TEMP ##########################################
        final Switch switch1 = (Switch) findViewById(R.id.switchTemp);
        final EditText currentTempSet = (EditText) findViewById(R.id.currentTempSet);
        final Button buttonPlusTemp = (Button) findViewById(R.id.buttonPlusTemp);
        final Button buttonMinusTemp = (Button) findViewById(R.id.buttonMinusTemp);

//################################# LIGHT #############################################
        final Switch switch2 = (Switch) findViewById(R.id.switchLight);
        final EditText currentLightOnSet = (EditText) findViewById(R.id.editcurrentLightOnSet);
        final EditText currentLightOffSet = (EditText) findViewById(R.id.editcurrentLightOffSet);
        final Button buttonPlusLightOn = (Button) findViewById(R.id.buttonPlusLightOn);
        final Button buttonMinusLightOn = (Button) findViewById(R.id.buttonMinusLightOn);
