        final Button buttonPlusHumid = (Button) findViewById(R.id.buttonPlusHumidity);
        final Button buttonMinusHumid = (Button) findViewById(R.id.buttonMinusHumidity);


        



        final ParseQuery<ParseObject> query = ParseQuery.getQuery("Greenhouse");
        query.getInBackground("CukTtkNoFt", new GetCallback<ParseObject>() {
            @Override
            public void done(final ParseObject object, ParseException e) {
                if (e == null) {

                    //###############################  TEMP ####################################

                    currentTempSet.setText((" ") + String.valueOf(object.getInt("setTemp")));
                    int tempOnOff = object.getInt("tempOnOff");
                    if (tempOnOff == 1) {
                        switch1.setChecked(true);
                    } else {
                        switch1.setChecked(false);
                    }

                    switch1.setOnClickListener(new View.OnClickListener() {
                        @Override
                        public void onClick(View view) {

                            String statusSwitch1;
                            if (switch1.isChecked()) {
                                statusSwitch1 = switch1.getTextOn().toString();
                                object.put("tempOnOff", 1);
                            } else {
                                statusSwitch1 = switch1.getTextOff().toString();
                                object.put("tempOnOff", 0);
                            }
                            object.put("index", 2);
                            object.saveInBackground();
                            Toast.makeText(Activity2.this, statusSwitch1, Toast.LENGTH_LONG).show();

                        }


                    });

                    buttonPlusTemp.setOnClickListener(new View.OnClickListener() {
                        @Override
                        public void onClick(View view) {

                            //String buttonPlus;
                            if (buttonPlusTemp.isPressed()) {
                                //clicked == true;
                                Integer x = (object.getInt("setTemp"));
                                Integer y = x + 1;
                                object.put("setTemp", y);
                                currentTempSet.setText((" ") + String.valueOf(object.getInt("setTemp")));
                                Toast.makeText(Activity2.this, "This Button is working " + y, Toast.LENGTH_SHORT).show();
                            } else {
                                //statusSwitch1 = switch1.getTextOff().toString();
                                //object.put("tempOnOff", "Off");
                                //object.put("index", 1);
                                //Toast.makeText(Activity2.this, buttonPlus, Toast.LENGTH_LONG).show();
                            }
                            object.put("index", 2);
                            object.saveInBackground();
                            //Toast.makeText(Activity2.this, buttonPlus, Toast.LENGTH_LONG).show();

                            }


                            });

                    buttonMinusTemp.setOnClickListener(new View.OnClickListener() {
                        @Override
                        public void onClick(View view) {

                            if (buttonMinusTemp.isPressed()) {
                                //clicked == true;
                                Integer x = (object.getInt("setTemp"));
                                Integer y = x - 1;
                                object.put("setTemp", y);
                                currentTempSet.setText((" ") + String.valueOf(object.getInt("setTemp")));
                                Toast.makeText(Activity2.this, "This Button is working " + y, Toast.LENGTH_SHORT).show();
                                } else {
                                //statusSwitch1 = switch1.getTextOff().toString();
                                //object.put("tempOnOff", "Off");
                                //object.put("index", 1);
                                //Toast.makeText(Activity2.this, buttonPlus, Toast.LENGTH_LONG).show();
                                }
                            object.put("index", 2);
                            object.saveInBackground();


                        }

                    });

                    //######################## LIGHT #########################################

                    currentLightOnSet.setText((" ") + String.valueOf(object.getInt("setLightOn")));
                    currentLightOffSet.setText((" ") + String.valueOf(object.getInt("setLightOff")));
                    int lightOnOff = object.getInt("lightOnOff");
                    if (lightOnOff == 1) {
                                switch2.setChecked(true);
                            } else {
                                switch2.setChecked(false);
                            }
                    switch2.setOnClickListener(new View.OnClickListener() {
                        @Override
                        public void onClick(View view) {

