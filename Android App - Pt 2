                    buttonMinusFAOff.setOnClickListener(new View.OnClickListener() {
                        @Override
                        public void onClick(View view) {

                            if (buttonMinusFAOff.isPressed()) {
                                //clicked == true;
                                Integer x = (object.getInt("setFAOn"));
                                Integer y = x - 1;
                                object.put("setFAOn", y);
                                currentFAOffSet.setText((" ") + String.valueOf(object.getInt("setFAOn")));
                                Toast.makeText(Activity2.this, "This Button is working " + y, Toast.LENGTH_SHORT).show();
                            } else {
                                //statusSwitch1 = switch1.getTextOff().toString();
                                //object.put("tempOnOff", "Off");
                                //object.put("index", 1);
                                //Toast.makeText(Activity2.this, buttonPlus, Toast.LENGTH_LONG).show();
                            }
                            object.put("index", 2);
                            object.saveInBackground();


                        }

                    });

                    //####################### HUMIDITY ##############################

                    currentHumidSet.setText((" ") + String.valueOf(object.getInt("setHumid")));
                    int humidOnOff = object.getInt("humidOnOff");
                    if (humidOnOff == 1) {
                        switch7.setChecked(true);
                    } else {
                        switch7.setChecked(false);
                    }

                    switch7.setOnClickListener(new View.OnClickListener() {
                        @Override
                        public void onClick(View view) {

                            String statusSwitch7;
                            if (switch7.isChecked()) {
                                statusSwitch7 = switch7.getTextOn().toString();
                                object.put("humidOnOff", 1);
                            } else {
                                statusSwitch7 = switch7.getTextOff().toString();
                                object.put("humidOnOff", 0);
                            }
                            object.put("index", 2);
                            object.saveInBackground();
                            Toast.makeText(Activity2.this, statusSwitch7, Toast.LENGTH_LONG).show();

                        }


                    });

                    buttonPlusHumid.setOnClickListener(new View.OnClickListener() {
                        @Override
                        public void onClick(View view) {

                            //String buttonPlus;
                            if (buttonPlusHumid.isPressed()) {
                                //clicked == true;
                                Integer x = (object.getInt("setHumid"));
                                Integer y = x + 1;
                                object.put("setHumid", y);
                                currentHumidSet.setText((" ") + String.valueOf(object.getInt("setHumid")));
                                Toast.makeText(Activity2.this, "This Button is working " + y, Toast.LENGTH_SHORT).show();
                            } else {
                                //statusSwitch1 = switch1.getTextOff().toString();
                                //object.put("tempOnOff", "Off");
                                //object.put("index", 1);
                                //Toast.makeText(Activity2.this, buttonPlus, Toast.LENGTH_LONG).show();
                            }
                            object.put("index", 2);
                            object.saveInBackground();
                            //Toast.makeText(Activity2.this, buttonPlus, Toast.LENGTH_LONG).show();

                        }


                    });

                    buttonMinusHumid.setOnClickListener(new View.OnClickListener() {
                        @Override
                        public void onClick(View view) {

                            if (buttonMinusHumid.isPressed()) {
                                //clicked == true;
                                Integer x = (object.getInt("setHumid"));
                                Integer y = x - 1;
                                object.put("setHumid", y);
                                currentHumidSet.setText((" ") + String.valueOf(object.getInt("setHumid")));
                                Toast.makeText(Activity2.this, "This Button is working " + y, Toast.LENGTH_SHORT).show();
                            } else {
                                //statusSwitch1 = switch1.getTextOff().toString();
                                //object.put("tempOnOff", "Off");
                                //object.put("index", 1);
                                //Toast.makeText(Activity2.this, buttonPlus, Toast.LENGTH_LONG).show();
                            }
                            object.put("index", 2);
                            object.saveInBackground();


                        }

                    });



                }
            }

            });
        }
    }









